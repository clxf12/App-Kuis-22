<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:padding="0dp"
	android:background="@drawable/bg"
	tools:context=".DaftarActivity">

	<ImageView
		android:layout_width="118dp"
		android:layout_height="124dp"
		android:layout_alignParentLeft="true"
		android:layout_alignParentTop="true"
		android:src="@drawable/simbol"
		android:id="@+id/log1daf"/>

	<ImageView
		android:layout_width="306dp"
		android:layout_height="39dp"
		android:src="@drawable/txt_daftar"
		android:layout_centerHorizontal="true"
		android:layout_below="@+id/log1daf"
		android:layout_marginTop="20dp"
		android:id="@+id/daf1" />

	<TextView
		android:layout_width="50sp"
		android:layout_height="wrap_content"
		android:layout_below="@+id/daf1"
		android:layout_marginLeft="56dp"
		android:layout_marginTop="20dp"
		android:textColor="@color/liteblack"
		android:fontFamily="@font/modak"
		android:textSize="18sp"
		android:text="Nama"
		android:id="@+id/nama1daf"/>


	<ImageView
		android:layout_width="300dp"
		android:layout_height="50dp"
		android:src="@drawable/kol_l"
		android:layout_below="@id/nama1daf"
		android:layout_marginTop="-3dp"
		android:layout_centerHorizontal="true"
		android:layout_centerInParent="true"
		android:id="@+id/passtxt1"/>

	<EditText
		android:layout_width="300dp"
		android:layout_height="50dp"
		android:layout_below="@+id/nama1daf"
		android:layout_centerHorizontal="true"
		android:textColor="@color/liteblack"
		android:background="@color/transparent"
		android:layout_marginLeft="16dp"
		android:layout_marginTop="-5dp"
		android:padding="16dp"
		android:maxLength="15"
		android:textColorHint="@color/grey"
		android:id="@+id/ktxtnamadaf"/>

	<TextView
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_below="@+id/passtxt1"
		android:layout_marginLeft="56dp"
		android:layout_marginTop="6dp"
		android:textColor="@color/liteblack"
		android:fontFamily="@font/modak"
		android:textSize="18sp"
		android:text="Jenis Kelamin"
		android:id="@+id/jnskdaf"/>


	<ImageView
		android:layout_width="300dp"
		android:layout_height="50dp"
		android:src="@drawable/kol_l"
		android:layout_below="@id/jnskdaf"
		android:layout_centerHorizontal="true"
		android:id="@+id/passtxt2"/>

	<EditText
		android:layout_width="300dp"
		android:layout_height="50dp"
		android:layout_below="@+id/jnskdaf"
		android:layout_centerHorizontal="true"
		android:textColor="@color/liteblack"
		android:background="@color/transparent"
		android:maxLength="9"
		android:layout_marginLeft="15dp"
		android:layout_marginTop="-3dp"
		android:padding="16dp"
		android:id="@+id/ktxtjnskdaf"/>

	<TextView
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_below="@+id/ktxtjnskdaf"
		android:layout_marginLeft="56dp"
		android:layout_marginTop="6dp"
		android:textColor="@color/liteblack"
		android:fontFamily="@font/modak"
		android:textSize="18sp"
		android:text="Masukan ID"
		android:id="@+id/userdaf"/>


	<ImageView
		android:layout_width="300dp"
		android:layout_height="50dp"
		android:src="@drawable/kol_l"
		android:layout_below="@id/userdaf"
		android:layout_centerHorizontal="true"
		android:id="@+id/passtxt3"/>

	<EditText
		android:layout_width="300dp"
		android:layout_height="50dp"
		android:layout_below="@+id/userdaf"
		android:layout_centerHorizontal="true"
		android:textColor="@color/liteblack"
		android:background="@color/transparent"
		android:layout_marginLeft="15dp"
		android:layout_marginTop="-3dp"
		android:maxLength="6"
		android:padding="16dp"
		android:id="@+id/ktxtuserdaf"/>

	<TextView
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_below="@+id/ktxtuserdaf"
		android:layout_marginLeft="56dp"
		android:layout_marginTop="6dp"
		android:textColor="@color/liteblack"
		android:fontFamily="@font/modak"
		android:textSize="18sp"
		android:text="Password"
		android:id="@+id/passwdaf"/>

	<ImageView
		android:layout_width="300dp"
		android:layout_height="50dp"
		android:src="@drawable/kol_l"
		android:layout_below="@id/passwdaf"
		android:layout_centerHorizontal="true"
		android:id="@+id/passtxt4"/>

		<EditText
			android:id="@+id/ktxtpassdaf"
			android:layout_width="270dp"
			android:layout_height="50dp"
			android:layout_below="@+id/passtxt4"
			android:background="@color/transparent"
			android:layout_centerHorizontal="true"
			android:layout_marginTop="-55dp"
			android:inputType="textPassword"
			android:drawableRight="@drawable/pass_hidden"
			android:maxLength="15"
			android:drawableTint="@color/grey"
			android:textColor="@color/liteblack" />

	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="horizontal">

		<ImageView
			android:id="@+id/bck_daf"
			android:layout_width="166dp"
			android:layout_height="39dp"
			android:layout_marginLeft="19dp"
			android:layout_marginTop="570dp"
			android:src="@drawable/kol_s" />

		<TextView
			android:id="@+id/bckdaftxt"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="52dp"
			android:layout_marginTop="572dp"
			android:fontFamily="@font/modak"
			android:text="Sebelumnya"
			android:textColor="@color/liteblack"
			android:textSize="20sp" />

		<ImageView
			android:id="@+id/sv_daf"
			android:layout_width="166dp"
			android:layout_height="39dp"
			android:layout_marginLeft="206dp"
			android:layout_marginTop="570dp"
			android:src="@drawable/kol_s" />

		<TextView
			android:id="@+id/tsvdafxt"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="260dp"
			android:layout_marginTop="572dp"
			android:fontFamily="@font/modak"
			android:textColor="@color/liteblack"
			android:text="Simpan"
			android:textSize="20sp" />


	</RelativeLayout>


</RelativeLayout>